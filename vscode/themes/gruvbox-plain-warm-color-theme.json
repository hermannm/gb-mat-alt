{
  "name": "gruvbox-plain-warm",
  "type": "dark",
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "variables, properties, plain text",
      "settings": { "foreground": "#ebdbb2" },
      "scope": [
        "entity.name.tag.yaml",
        "entity.other.attribute-name",
        "support.constant.property",
        "support.type.property-name",
        "variable",
        "variable.other.enummember.c"
      ]
    },
    {
      "name": "keywords, operators",
      "settings": { "foreground": "#fe8019" },
      "scope": [
        "constant.other.placeholder",
        "keyword.operator",
        "keyword.type.graphql",
        "keyword",
        "punctuation.definition.annotation",
        "punctuation.definition.decorator",
        "punctuation.definition.entity.css",
        "punctuation.definition.heading.markdown",
        "punctuation.definition.italic.markdown",
        "punctuation.definition.list.begin.markdown",
        "punctuation.separator.key-value.html",
        "punctuation.separator.pointer-access",
        "storage.modifier",
        "storage.type"
      ]
    },
    {
      "name": "types, classes, namespaces, components, tags",
      "settings": { "foreground": "#fabd2f" },
      "scope": [
        "entity.name.namespace",
        "entity.name.tag",
        "entity.name.type",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.pseudo-element.css",
        "keyword.type",
        "punctuation.definition.lifetime",
        "storage.modifier.lifetime",
        "storage.type.annotation",
        "storage.type.built-in.primitive",
        "storage.type.cs",
        "storage.type.primitive",
        "support.class",
        "support.type",
        "variable.other.enummember"
      ]
    },
    {
      "name": "functions, methods",
      "settings": { "foreground": "#b8bb26" },
      "scope": ["entity.name.function", "markup.underline.link.markdown", "support.function"]
    },
    {
      "name": "values, language constants",
      "settings": { "foreground": "#8ec07c" },
      "scope": [
        "constant.language",
        "constant.numeric",
        "constant.other.color",
        "entity.name.function.preprocessor",
        "keyword.other.unit",
        "markup.fenced_code.block.markdown",
        "markup.inline.raw.string.markdown",
        "punctuation.definition.markdown",
        "punctuation.definition.raw.markdown",
        "string",
        "support.constant",
        "variable.language",
        "variable.parameter.function.language.special.self"
      ]
    },
    {
      "name": "punctuation",
      "settings": { "foreground": "#a89984" },
      "scope": [
        "constant.character.escape.continuation",
        "constant.character.format.placeholder.other",
        "keyword.operator.access",
        "keyword.operator.accessor",
        "keyword.operator.key-value",
        "keyword.operator.namespace",
        "keyword.operator.type.annotation",
        "meta.brace",
        "punctuation.accessor",
        "punctuation.bracket",
        "punctuation.brackets",
        "punctuation.colon",
        "punctuation.comma",
        "punctuation.curlybrace",
        "punctuation.definition.arguments",
        "punctuation.definition.array",
        "punctuation.definition.attribute",
        "punctuation.definition.begin.bracket",
        "punctuation.definition.binding-pattern",
        "punctuation.definition.block",
        "punctuation.definition.bracket",
        "punctuation.definition.dict",
        "punctuation.definition.dictionary",
        "punctuation.definition.end.bracket",
        "punctuation.definition.group",
        "punctuation.definition.imports",
        "punctuation.definition.inheritance",
        "punctuation.definition.list",
        "punctuation.definition.logical-expression",
        "punctuation.definition.parameters",
        "punctuation.definition.section",
        "punctuation.definition.sequence",
        "punctuation.definition.tag",
        "punctuation.definition.template-expression",
        "punctuation.definition.typeparameters",
        "punctuation.definition.variable.makefile",
        "punctuation.definition.variable.shell",
        "punctuation.definition.variables",
        "punctuation.destructuring",
        "punctuation.operation.graphql",
        "punctuation.other.colon",
        "punctuation.other.comma",
        "punctuation.other.period",
        "punctuation.parenthesis",
        "punctuation.section.embedded",
        "punctuation.section.function",
        "punctuation.section",
        "punctuation.semi",
        "punctuation.separator",
        "punctuation.squarebracket",
        "punctuation.terminator"
      ]
    },
    {
      "name": "comments, documentation",
      "settings": { "foreground": "#928374" },
      "scope": [
        "comment entity.name.tag",
        "comment",
        "punctuation.definition.tag",
        "string.quoted.docstring"
      ]
    },
    {
      "name": "italic",
      "settings": { "fontStyle": "italic" },
      "scope": ["markup.italic.markdown"]
    },
    {
      "name": "bold",
      "settings": { "fontStyle": "bold" },
      "scope": ["markup.bold.markdown"]
    }
  ],
  "semanticTokenColors": {
    "builtinAttribute": "#fabd2f",
    "decorator": "#fabd2f",
    "generic.attribute": "#fabd2f",
    "operatorOverloaded": "#fe8019",
