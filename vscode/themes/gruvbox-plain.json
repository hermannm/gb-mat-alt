{
	"name": "gruvbox-plain",
	"type": "dark",
	"semanticHighlighting": true,
	"tokenColors": [
    {
			"name": "variables, properties, plain text",
			"settings": {"foreground": "#ebdbb2"},
			"scope": [
				"variable",
				"entity.other.attribute-name",
				"support.type.property-name"
			]
		},
		{
			"name": "keywords, operators",
			"settings": {"foreground": "#fe8019"},
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",

        "keyword.operator",
        "punctuation.definition.decorator",
        "punctuation.definition.annotation",

        "punctuation.definition.heading.markdown",
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.italic.markdown"
			]
		},
    {
			"name": "types, classes, components, tags",
			"settings": {"foreground": "#fabd2f"},
			"scope": [
				"entity.name.type",
				"entity.name.namespace",
        "entity.name.tag",
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.id.css",
				"support.type",
				"support.class",
        "storage.type.primitive",
        "storage.type.annotation"
			]
		},
    {
			"name": "functions, methods",
			"settings": {"foreground": "#b8bb26"},
			"scope": [
				"entity.name.function",
				"support.function",

        "markup.underline.link.markdown"
			]
		},
    {
			"name": "values, language constants",
			"settings": {"foreground": "#8ec07c"},
			"scope": [
				"string",
				"constant.numeric",
				"constant.language",
				"support.constant",
        "variable.parameter.function.language",
        "meta.jsx.children",
        
				"keyword.other.unit",
				"constant.other.color",

        "markup.inline.raw.string.markdown",
        "markup.fenced_code.block.markdown",
        "punctuation.definition.markdown",
        "punctuation.definition.raw.markdown"
			]
		},
		{
			"name": "punctuation",
			"settings": {"foreground": "#a89984"},
			"scope": [
        "punctuation.accessor",
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.parenthesis",
        "punctuation.curlybrace",
        "punctuation.squarebracket",
        "punctuation.bracket",
        "punctuation.section",
        "punctuation.other.period",
        "punctuation.other.comma",
        "punctuation.other.colon",
				"punctuation.definition.block",
        "punctuation.definition.parameters",
        "punctuation.definition.arguments",
        "punctuation.definition.binding-pattern",
        "punctuation.definition.tag",
        "punctuation.definition.template-expression",
        "punctuation.definition.begin.bracket",
        "punctuation.definition.end.bracket",
        "punctuation.definition.bracket",
        "punctuation.definition.list",
        "punctuation.definition.inheritance",
        "punctuation.definition.typeparameters",
        "punctuation.definition.dictionary",
        "punctuation.definition.dict",
        "punctuation.definition.array",
        "punctuation.section.embedded",
        "punctuation.section.function",
        "keyword.operator.type.annotation",
        "constant.character.format.placeholder.other",
        "meta.brace"
			]
		},
		{
			"name": "comments, documentation",
			"settings": {"foreground": "#928374"},
			"scope": [
				"comment",
				"string.quoted.docstring",
				"punctuation.definition.tag"
			]
		},
    {
      "name": "italic",
      "settings": {"fontStyle": "italic"},
      "scope": [
        "markup.italic.markdown"
      ]
    },
    {
      "name": "bold",
      "settings": {"fontStyle": "bold"},
      "scope": [
        "markup.bold.markdown"
      ]
    }
	],
  "semanticTokenColors": {
    "operatorOverloaded": "#fe8019"
  },
